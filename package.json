{
  "name": "cv-builder",
  "version": "1.0.0",
  "description": "CV Builder Application using React, Express, and NodeJS with Multi Step Form and PDF Generator",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run cleint\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salamidrus/cv-builder.git"
  },
