  "bugs": {
    "url": "https://github.com/salamidrus/cv-builder/issues"
  },
  "homepage": "https://github.com/salamidrus/cv-builder#readme",
  "dependencies": {
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "html-pdf": "^2.2.0"
  }
}
